1. SELECT * FROM EMPLOYEES
   WHERE PHONE_NUMBER
   LIKE '%90';

2. SELECT * FROM EMPLOYEES
   WHERE EMAIL
   LIKE '%GMAIL';

3. SELECT * FROM EMPLOYEES
   WHERE DEPARTMENT_ID = 5 AND DEPARTMENT_ID = 6; #
   SELECT CONCAT ('5',' ','6') AS DEPARTMENT FROM DEPARTMENT_ID.EMPLOYEES;

4. SELECT SALARY FROM EMPLOYEES
   WHERE SALARY > 5000; #  SELECT * FROM EMPLOYEES WHERE SALARY > 5000;

5. SELECT SALARY FROM EMPLOYEES
   WHERE SALARY > 2000 AND SALARY < 1500;
   #  SELECT * FROM EMPLOYEES WHERE SALARY > 2000 AND SALARY < 1500;